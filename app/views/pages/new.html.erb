<%= form_for @project , url: {action: "create"} do |f| %>
 <%= f.label 'Project Name:' %>
 <%= f.text_field :name, autocomplete: 'off' %><br><br>
   <%= f.fields_for :tasks, @project.tasks.build do |ff| %>
    <%= ff.label 'Project Title:' %>
    <%= ff.text_field :title , autocomplete: 'off' %><br><br>
   <% end %>
 <%= f.submit %>
<% end %>
